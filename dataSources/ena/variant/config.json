{
    "dbType": "location",
    "dataLocation": "https://ftp.ebi.ac.uk/pub/databases/eva/rs_releases/release_4/by_species/",
    "subDirectoryDepth": -1,
    "regexMatch": ".*\\.txt\\.gz",
    "perFileProcessing": [
        {
            "path": "lib/tools/extractor.py",
            "function": "Extractor.extract",
            "args": [
                "{INPUT, 0, FILEPATH}",
                "{PATH, PROCESSING}"
            ],
            "outputs": [
                "{INPUTPATH, PROCESSING, 0, FILEPATH_STEM}"
            ]
        }
    ],
    "finalProcessing": [
        {
            "path": "sourceProcessing/ena.py",
            "function": "compile",
            "args": [
                "{PATH, PROCESSING}",
                "{PATH, PREDWC, enaVariant.csv}"
            ],
            "outputs": [
                "{PATH, PREDWC, enaVariant.csv}"
            ]
        }
    ]
}
